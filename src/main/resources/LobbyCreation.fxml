<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19">
    <children>
        <Label fx:id="creationLabel" alignment="CENTER" layoutY="14.0" prefHeight="17.0" prefWidth="600.0"
               text="Choose the lobby capacity">
            <font>
                <Font size="32.0"/>
            </font>
        </Label>
        <ComboBox fx:id="lobbyCapacity" layoutX="225.0" layoutY="109.0" prefWidth="150.0"/>
        <Button fx:id="selectCapacity" layoutX="225.0" layoutY="168.0" mnemonicParsing="false" onAction="#setCapacity"
                prefHeight="65.0" prefWidth="150.0" text="Create"/>
    </children>
</AnchorPane>
